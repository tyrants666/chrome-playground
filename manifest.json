{
    "manifest_version": 3,
    "name": "Chrome Playground",
    "version": "0.0.1",
    "description": "Intuitive way to start your chrome projects",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {                
        "128": "icons/logo.png"
      }
    },
    "host_permissions": [
      "https://developer.chrome.com/*"
    ],
    "permissions": [
      "tabs",
      "tabGroups",
      "storage"
    ]
}
